admin   off

<% sites.forEach(site => { %>
:<%= site.port %> {
    root "<%= site.root -%>"
    <% if(site.index) { %>
    index <%= site.index -%>
    <% } %>
    file_server
}

:<%= site.port %>/environment.json {
    root "<%= envDir %>"
    templates
    rewrite /environment.json /<%= site.id %>.json
    file_server
}
<% }) %>
